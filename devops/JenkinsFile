pipeline {
    agent any
    stages {
        stage('Checkout development branch of DWH repository to Jenkins Machine') {
            steps 
            {
                git branch: 'feature/s1', url: 'https://github.com/olcayozz/bugglesLab.git'
                echo "2"
            }
        }
        stage('Parse ChangeSet') {
            steps {
                script {
                    echo "3"
                    COMMITTED_PATHS = sh (script: 'git diff-tree --no-commit-id --name-only -r HEAD', returnStdout: true).trim()
                    echo "${COMMITTED_PATHS}\n"
                }
            }
        }
    }
}
